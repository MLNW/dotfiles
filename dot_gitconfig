[user]
    name = Lucas Resch
    email = lucas.resch@gmx.de

[branch]
    sort = -committerdate

[core]
    autocrlf = false
    editor = vim
    longpaths = true
    commentChar = ";"

[commit]
    verbose = true

[diff]
    algorithm = histogram

[fetch]
    prune = true

[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true

[init]
    defaultBranch = trunk

[pull]
    ff = only

[push]
    autoSetupRemote = true

[rerere]
    enabled = true

